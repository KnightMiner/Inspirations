{
    "multipart": [
        {   "when": { "northwest": "true" },
            "apply": { "model": "inspirations:block/carpet/lower/lime" }
        },
        {   "when": { "northwest": "false" },
            "apply": { "model": "inspirations:block/carpet/corner/lime" }
        },
        {   "when": { "northwest": "false", "northeast": "true" },
            "apply": { "model": "inspirations:block/carpet/wall_right/lime" }
        },
        {   "when": { "northwest": "false", "southwest": "true" },
            "apply": { "model": "inspirations:block/carpet/wall_left/lime" }
        },
        {   "when": { "northwest": "false", "northeast": "true", "southwest": "true", "southeast": "true" },
            "apply": { "model": "inspirations:block/carpet/wall_post/lime" }
        },
        
        {   "when": { "northeast": "true" },
            "apply": { "model": "inspirations:block/carpet/lower/lime", "y": 90, "uvlock": true }
        },
        {   "when": { "northeast": "false" },
            "apply": { "model": "inspirations:block/carpet/corner/lime", "y": 90, "uvlock": true }
        },
        {   "when": { "northeast": "false", "southeast": "true" },
            "apply": { "model": "inspirations:block/carpet/wall_right/lime", "y": 90, "uvlock": true }
        },
        {   "when": { "northeast": "false", "northwest": "true" },
            "apply": { "model": "inspirations:block/carpet/wall_left/lime", "y": 90, "uvlock": true }
        },
        {   "when": { "northeast": "false", "northwest": "true", "southwest": "true", "southeast": "true" },
            "apply": { "model": "inspirations:block/carpet/wall_post/lime", "y": 90, "uvlock": true }
        },
        
        {   "when": { "southeast": "true" },
            "apply": { "model": "inspirations:block/carpet/lower/lime", "y": 180, "uvlock": true }
        },
        {   "when": { "southeast": "false" },
            "apply": { "model": "inspirations:block/carpet/corner/lime", "y": 180, "uvlock": true }
        },
        {   "when": { "southeast": "false", "southwest": "true" },
            "apply": { "model": "inspirations:block/carpet/wall_right/lime", "y": 180, "uvlock": true }
        },
        {   "when": { "southeast": "false", "northeast": "true" },
            "apply": { "model": "inspirations:block/carpet/wall_left/lime", "y": 180, "uvlock": true }
        },
        {   "when": { "southeast": "false", "northeast": "true", "southwest": "true", "northwest": "true" },
            "apply": { "model": "inspirations:block/carpet/wall_post/lime", "y": 180, "uvlock": true }
        },
        
        {   "when": { "southwest": "true" },
            "apply": { "model": "inspirations:block/carpet/lower/lime", "y": 270, "uvlock": true }
        },
        {   "when": { "southwest": "false" },
            "apply": { "model": "inspirations:block/carpet/corner/lime", "y": 270, "uvlock": true }
        },
        {   "when": { "southwest": "false", "northwest": "true" },
            "apply": { "model": "inspirations:block/carpet/wall_right/lime", "y": 270, "uvlock": true }
        },
        {   "when": { "southwest": "false", "southeast": "true" },
            "apply": { "model": "inspirations:block/carpet/wall_left/lime", "y": 270, "uvlock": true }
        },
        {   "when": { "southwest": "false", "northeast": "true", "northwest": "true", "southeast": "true" },
            "apply": { "model": "inspirations:block/carpet/wall_post/lime", "y": 270, "uvlock": true }
        },
        
        {   "when": { "southwest": "true", "southeast": "true" },
            "apply": { "model": "inspirations:block/carpet/front/lime" }
        },
        {   "when": { "northwest": "true", "southwest": "true" },
            "apply": { "model": "inspirations:block/carpet/front/lime", "y": 90, "uvlock": true }
        },
        {   "when": { "northwest": "true", "northeast": "true" },
            "apply": { "model": "inspirations:block/carpet/front/lime", "y": 180, "uvlock": true }
        },
        {   "when": { "northeast": "true", "southeast": "true" },
            "apply": { "model": "inspirations:block/carpet/front/lime", "y": 270, "uvlock": true }
        },
        
        {   "when": { "northeast": "true", "southwest": "true", "southeast": "true" },
            "apply": { "model": "inspirations:block/carpet/front_post/lime" }
        },
        {   "when": { "northwest": "true", "southwest": "true", "southeast": "true" },
            "apply": { "model": "inspirations:block/carpet/front_post/lime", "y": 90, "uvlock": true }
        },
        {   "when": { "northwest": "true", "northeast": "true", "southwest": "true" },
            "apply": { "model": "inspirations:block/carpet/front_post/lime", "y": 180, "uvlock": true }
        },
        {   "when": { "northwest": "true", "northeast": "true", "southeast": "true" },
            "apply": { "model": "inspirations:block/carpet/front_post/lime", "y": 270, "uvlock": true }
        }
    ]
