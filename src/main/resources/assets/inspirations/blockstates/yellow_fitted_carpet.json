{
    "multipart": [
        {   "when": { "northwest": "true" },
            "apply": { "model": "inspirations:block/carpet/lower/yellow" }
        },
        {   "when": { "northwest": "false" },
            "apply": { "model": "inspirations:block/carpet/corner/yellow" }
        },
        {   "when": { "northwest": "false", "northeast": "true" },
            "apply": { "model": "inspirations:block/carpet/wall_right/yellow" }
        },
        {   "when": { "northwest": "false", "southwest": "true" },
            "apply": { "model": "inspirations:block/carpet/wall_left/yellow" }
        },
        {   "when": { "northwest": "false", "northeast": "true", "southwest": "true", "southeast": "true" },
            "apply": { "model": "inspirations:block/carpet/wall_post/yellow" }
        },
        
        {   "when": { "northeast": "true" },
            "apply": { "model": "inspirations:block/carpet/lower/yellow", "y": 90, "uvlock": true }
        },
        {   "when": { "northeast": "false" },
            "apply": { "model": "inspirations:block/carpet/corner/yellow", "y": 90, "uvlock": true }
        },
        {   "when": { "northeast": "false", "southeast": "true" },
            "apply": { "model": "inspirations:block/carpet/wall_right/yellow", "y": 90, "uvlock": true }
        },
        {   "when": { "northeast": "false", "northwest": "true" },
            "apply": { "model": "inspirations:block/carpet/wall_left/yellow", "y": 90, "uvlock": true }
        },
        {   "when": { "northeast": "false", "northwest": "true", "southwest": "true", "southeast": "true" },
            "apply": { "model": "inspirations:block/carpet/wall_post/yellow", "y": 90, "uvlock": true }
        },
        
        {   "when": { "southeast": "true" },
            "apply": { "model": "inspirations:block/carpet/lower/yellow", "y": 180, "uvlock": true }
        },
        {   "when": { "southeast": "false" },
            "apply": { "model": "inspirations:block/carpet/corner/yellow", "y": 180, "uvlock": true }
        },
        {   "when": { "southeast": "false", "southwest": "true" },
            "apply": { "model": "inspirations:block/carpet/wall_right/yellow", "y": 180, "uvlock": true }
        },
        {   "when": { "southeast": "false", "northeast": "true" },
            "apply": { "model": "inspirations:block/carpet/wall_left/yellow", "y": 180, "uvlock": true }
        },
        {   "when": { "southeast": "false", "northeast": "true", "southwest": "true", "northwest": "true" },
            "apply": { "model": "inspirations:block/carpet/wall_post/yellow", "y": 180, "uvlock": true }
        },
        
        {   "when": { "southwest": "true" },
            "apply": { "model": "inspirations:block/carpet/lower/yellow", "y": 270, "uvlock": true }
        },
        {   "when": { "southwest": "false" },
            "apply": { "model": "inspirations:block/carpet/corner/yellow", "y": 270, "uvlock": true }
        },
        {   "when": { "southwest": "false", "northwest": "true" },
            "apply": { "model": "inspirations:block/carpet/wall_right/yellow", "y": 270, "uvlock": true }
        },
        {   "when": { "southwest": "false", "southeast": "true" },
            "apply": { "model": "inspirations:block/carpet/wall_left/yellow", "y": 270, "uvlock": true }
        },
        {   "when": { "southwest": "false", "northeast": "true", "northwest": "true", "southeast": "true" },
            "apply": { "model": "inspirations:block/carpet/wall_post/yellow", "y": 270, "uvlock": true }
        },
        
        {   "when": { "southwest": "true", "southeast": "true" },
            "apply": { "model": "inspirations:block/carpet/front/yellow" }
        },
        {   "when": { "northwest": "true", "southwest": "true" },
            "apply": { "model": "inspirations:block/carpet/front/yellow", "y": 90, "uvlock": true }
        },
        {   "when": { "northwest": "true", "northeast": "true" },
            "apply": { "model": "inspirations:block/carpet/front/yellow", "y": 180, "uvlock": true }
        },
        {   "when": { "northeast": "true", "southeast": "true" },
            "apply": { "model": "inspirations:block/carpet/front/yellow", "y": 270, "uvlock": true }
        },
        
        {   "when": { "northeast": "true", "southwest": "true", "southeast": "true" },
            "apply": { "model": "inspirations:block/carpet/front_post/yellow" }
