            "apply": { "model": "inspirations:block/carpet/wall_post/orange" }
        },
        
        {   "when": { "northeast": "true" },
            "apply": { "model": "inspirations:block/carpet/lower/orange", "y": 90, "uvlock": true }
        },
        {   "when": { "northeast": "false" },
