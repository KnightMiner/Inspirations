            "apply": { "model": "inspirations:block/carpet/wall_post/orange", "y": 270, "uvlock": true }
        },
        
        {   "when": { "southwest": "true", "southeast": "true" },
            "apply": { "model": "inspirations:block/carpet/front/orange" }
