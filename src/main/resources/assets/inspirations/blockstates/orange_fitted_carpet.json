            "apply": { "model": "inspirations:block/carpet/wall_post/orange", "y": 180, "uvlock": true }
        },
        
        {   "when": { "southwest": "true" },
            "apply": { "model": "inspirations:block/carpet/lower/orange", "y": 270, "uvlock": true }
        },
        {   "when": { "southwest": "false" },
