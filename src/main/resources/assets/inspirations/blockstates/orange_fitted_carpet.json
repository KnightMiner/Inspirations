{
    "multipart": [
        {   "when": { "northwest": "true" },
            "apply": { "model": "inspirations:block/carpet/lower/orange" }
        },
        {   "when": { "northwest": "false" },
            "apply": { "model": "inspirations:block/carpet/corner/orange" }
        },
        {   "when": { "northwest": "false", "northeast": "true" },
            "apply": { "model": "inspirations:block/carpet/wall_right/orange" }
