            "apply": { "model": "inspirations:block/carpet/wall_post/white", "y": 180, "uvlock": true }
        },
        
        {   "when": { "southwest": "true" },
            "apply": { "model": "inspirations:block/carpet/lower/white", "y": 270, "uvlock": true }
        },
        {   "when": { "southwest": "false" },
            "apply": { "model": "inspirations:block/carpet/corner/white", "y": 270, "uvlock": true }
        },
        {   "when": { "southwest": "false", "northwest": "true" },
            "apply": { "model": "inspirations:block/carpet/wall_right/white", "y": 270, "uvlock": true }
        },
        {   "when": { "southwest": "false", "southeast": "true" },
            "apply": { "model": "inspirations:block/carpet/wall_left/white", "y": 270, "uvlock": true }
        },
        {   "when": { "southwest": "false", "northeast": "true", "northwest": "true", "southeast": "true" },
            "apply": { "model": "inspirations:block/carpet/wall_post/white", "y": 270, "uvlock": true }
        },
        
        {   "when": { "southwest": "true", "southeast": "true" },
            "apply": { "model": "inspirations:block/carpet/front/white" }
        },
        {   "when": { "northwest": "true", "southwest": "true" },
            "apply": { "model": "inspirations:block/carpet/front/white", "y": 90, "uvlock": true }
        },
        {   "when": { "northwest": "true", "northeast": "true" },
            "apply": { "model": "inspirations:block/carpet/front/white", "y": 180, "uvlock": true }
        },
        {   "when": { "northeast": "true", "southeast": "true" },
            "apply": { "model": "inspirations:block/carpet/front/white", "y": 270, "uvlock": true }
