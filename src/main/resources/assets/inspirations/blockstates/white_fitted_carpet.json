            "apply": { "model": "inspirations:block/carpet/front_post/white", "y": 180, "uvlock": true }
        },
        {   "when": { "northwest": "true", "northeast": "true", "southeast": "true" },
            "apply": { "model": "inspirations:block/carpet/front_post/white", "y": 270, "uvlock": true }
        }
    ]
