            "apply": { "model": "inspirations:block/carpet/wall_left/white", "y": 90, "uvlock": true }
        },
        {   "when": { "northeast": "false", "northwest": "true", "southwest": "true", "southeast": "true" },
            "apply": { "model": "inspirations:block/carpet/wall_post/white", "y": 90, "uvlock": true }
        },
        
        {   "when": { "southeast": "true" },
            "apply": { "model": "inspirations:block/carpet/lower/white", "y": 180, "uvlock": true }
        },
        {   "when": { "southeast": "false" },
            "apply": { "model": "inspirations:block/carpet/corner/white", "y": 180, "uvlock": true }
        },
        {   "when": { "southeast": "false", "southwest": "true" },
            "apply": { "model": "inspirations:block/carpet/wall_right/white", "y": 180, "uvlock": true }
