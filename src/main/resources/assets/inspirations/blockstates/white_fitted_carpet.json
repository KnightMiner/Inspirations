{
    "multipart": [
        {   "when": { "northwest": "true" },
            "apply": { "model": "inspirations:block/carpet/lower/white" }
        },
        {   "when": { "northwest": "false" },
            "apply": { "model": "inspirations:block/carpet/corner/white" }
        },
        {   "when": { "northwest": "false", "northeast": "true" },
            "apply": { "model": "inspirations:block/carpet/wall_right/white" }
        },
        {   "when": { "northwest": "false", "southwest": "true" },
            "apply": { "model": "inspirations:block/carpet/wall_left/white" }
        },
        {   "when": { "northwest": "false", "northeast": "true", "southwest": "true", "southeast": "true" },
            "apply": { "model": "inspirations:block/carpet/wall_post/white" }
        },
        
        {   "when": { "northeast": "true" },
            "apply": { "model": "inspirations:block/carpet/lower/white", "y": 90, "uvlock": true }
        },
        {   "when": { "northeast": "false" },
            "apply": { "model": "inspirations:block/carpet/corner/white", "y": 90, "uvlock": true }
        },
        {   "when": { "northeast": "false", "southeast": "true" },
